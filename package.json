		"@nextui-org/theme": "^2.2.11",
		"@react-aria/ssr": "^3.9.6",
		"@react-aria/visually-hidden": "^3.8.17",
		"@stripe/stripe-js": "^4.9.0",
		"@supabase/auth-helpers-nextjs": "^0.10.0",
