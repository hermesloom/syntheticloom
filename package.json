		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"email-validator": "^2.0.4",
		"framer-motion": "^11.11.11",
		"fs-extra": "^11.2.0",
		"intl-messageformat": "^10.7.4",
		"lodash": "^4.17.21",
		"next": "^15.0.2",
		"next-themes": "^0.3.0",
		"openai": "^4.70.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"stripe": "^17.3.1",
		"tsx": "^4.19.2",
		"zod": "^3.23.8"
